{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  
  "github": {
    "autoJobCancelation": true,
    "silent": true
  },
  
  "ignoreCommand": "bash vercel-build-filter.sh",
  
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}

